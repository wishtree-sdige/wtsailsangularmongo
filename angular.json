{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wtsailsangularmongo": {
      "projectType": "application",
      "schematics": {},
      "root": "assets/app",
      "sourceRoot": "assets/app",
      "prefix": "assets/app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "assets/app/dist/wtsailsangularmongo/browser",
            "index": "assets/app/index.html",
            "main": "assets/app/main.ts",
            "polyfills": "assets/app/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "assets/app/favicon.ico",
              "assets/app/assets"
            ],
            "styles": [
              "assets/app/custom-theme.scss",
              "assets/app/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "assets/app/environments/environment.ts",
                  "with": "assets/app/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "assets/app/environments/environment.ts",
                  "with": "assets/app/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "assets/app/environments/environment.ts",
                  "with": "assets/app/environments/environment.qa.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "fr":{
              "aot": true,
              "outputPath": "assets/app/dist/wtsailsangularmongo",
              "i18nFile": "assets/app/locals/messages.fr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "fr",
              "i18nMissingTranslation": "error"

            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wtsailsangularmongo:build",
            "port": 4000 
          },
          "configurations": {
            "production": {
              "browserTarget": "wtsailsangularmongo:build:production"
            },
            "staging": {
              "browserTarget": "wtsailsangularmongo:build:staging"
            },
            "qa": {
              "browserTarget": "wtsailsangularmongo:build:qa"
            },
            "fr":{
              "browserTarget": "wtsailsangularmongo:build:fr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wtsailsangularmongo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "assets/app/test.ts",
            "polyfills": "assets/app/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "assets/app/favicon.ico",
              "assets/app/assets"
            ],
            "styles": [
              "assets/app/styles.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/datatables.net/js/jquery.dataTables.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "wtsailsangularmongo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wtsailsangularmongo:serve:production"
            },
            "staging": {
              "devServerTarget": "wtsailsangularmongo:serve:staging"
            },
            "qa": {
              "devServerTarget": "wtsailsangularmongo:serve:qa"
            }
            
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "assets/app/dist/wtsailsangularmongo/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "assets/app/environments/environment.ts",
                  "with": "assets/app/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "wtsailsangularmongo:build",
            "serverTarget": "wtsailsangularmongo:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "wtsailsangularmongo:build:production",
              "serverTarget": "wtsailsangularmongo:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "wtsailsangularmongo:build:production",
            "serverTarget": "wtsailsangularmongo:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    }
  },
  "defaultProject": "wtsailsangularmongo",
  "cli": {
    "analytics": false
  }
}